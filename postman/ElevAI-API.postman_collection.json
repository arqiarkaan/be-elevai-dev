{
	"info": {
		"_postman_id": "elevai-api-2024",
		"name": "ElevAI Backend API",
		"description": "Complete API collection for ElevAI - Set authToken variable with your Supabase JWT",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"auth": {
		"type": "bearer",
		"bearer": [{"key": "token", "value": "{{authToken}}", "type": "string"}]
	},
	"variable": [
		{"key": "baseUrl", "value": "http://localhost:3000"},
		{"key": "authToken", "value": "YOUR_SUPABASE_JWT_TOKEN_HERE"}
	],
	"item": [
		{
			"name": "Health & Info",
			"item": [
				{
					"name": "Root Info",
					"request": {
						"auth": {"type": "noauth"},
						"method": "GET",
						"url": "{{baseUrl}}/"
					}
				},
				{
					"name": "Health Check",
					"request": {
						"auth": {"type": "noauth"},
						"method": "GET",
						"url": "{{baseUrl}}/health"
					}
				},
				{
					"name": "Get All Features",
					"request": {
						"auth": {"type": "noauth"},
						"method": "GET",
						"url": "{{baseUrl}}/features"
					}
				}
			]
		},
		{
			"name": "Payment",
			"item": [
				{
					"name": "Get Plans",
					"request": {
						"auth": {"type": "noauth"},
						"method": "GET",
						"url": "{{baseUrl}}/api/payment/plans"
					}
				},
				{
					"name": "Create Payment - Monthly",
					"request": {
						"method": "POST",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\"type\":\"subscription\",\"item\":\"monthly\",\"amount\":49000,\"tokens_amount\":200}"
						},
						"url": "{{baseUrl}}/api/payment/create"
					}
				},
				{
					"name": "Create Payment - Yearly",
					"request": {
						"method": "POST",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\"type\":\"subscription\",\"item\":\"yearly\",\"amount\":490000,\"tokens_amount\":2500}"
						},
						"url": "{{baseUrl}}/api/payment/create"
					}
				},
				{
					"name": "Buy Tokens - 50",
					"request": {
						"method": "POST",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\"type\":\"tokens\",\"item\":\"small\",\"amount\":15000,\"tokens_amount\":50}"
						},
						"url": "{{baseUrl}}/api/payment/create"
					}
				}
			]
		},
		{
			"name": "User",
			"item": [
				{
					"name": "Get Profile",
					"request": {"method": "GET", "url": "{{baseUrl}}/api/user/profile"}
				},
				{
					"name": "Get Tokens",
					"request": {"method": "GET", "url": "{{baseUrl}}/api/user/tokens"}
				},
				{
					"name": "Get Transactions",
					"request": {"method": "GET", "url": "{{baseUrl}}/api/user/transactions"}
				},
				{
					"name": "Get Usage",
					"request": {"method": "GET", "url": "{{baseUrl}}/api/user/usage"}
				},
				{
					"name": "Get Subscription",
					"request": {"method": "GET", "url": "{{baseUrl}}/api/user/subscription"}
				}
			]
		},
